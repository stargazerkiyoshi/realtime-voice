{
    "name":  "realtime-voice",
    "version":  "0.1.0",
    "private":  true,
    "type":  "commonjs",
    "scripts":  {
                    "dev":  "tsx watch src/server.ts",
                    "dev:example": "powershell -ExecutionPolicy Bypass -File scripts/start.example.ps1",
                    "dev:local": "powershell -ExecutionPolicy Bypass -File scripts/start.local.ps1",
                    "build":  "tsc -p tsconfig.json",
                    "start":  "node dist/server.js",
                    "test":  "node --import tsx --test tests/async-queue.test.ts tests/chunker.test.ts tests/asr-parse.test.ts",
                    "asr:diag": "node --import tsx scripts/asr.diagnose.ts"
                },
    "dependencies":  {
        "fastify":  "^4.28.1",
        "@fastify/websocket":  "^10.0.1",
        "ws":  "^8.17.0",
        "openai": "^4.54.1"
                     },
    "devDependencies":  {
                            "typescript":  "^5.4.5",
                            "tsx":  "^4.17.0",
                            "@types/node":  "^20.11.30",
                            "@types/ws":  "^8.5.12",
                            "ffmpeg-static": "^5.2.0"
                        }
}
